    "            fila = np.argmax(columna)\n",
    "            soluciones[col] = tabla[fila, -1]\n",
    "\n",
    "    Z_optimo = tabla[-1, -1]\n",
    "    print(\"\\nüîé Soluci√≥n √≥ptima encontrada:\")\n",
    "    for i, nombre in enumerate(nombres_variables):\n",
    "        print(f\"{nombre} = {soluciones[i]}\")\n",
    "    print(f\"Z = {Z_optimo}\")\n",
    "\n"
